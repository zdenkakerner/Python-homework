   "source": [
    "# given a list of numbers, go over them and check if they are odd\n",
    "a = 4303\n",
    "b = 9176\n",
    "result = 0\n",
    "for n in range(a, b+1):\n",
    "    # for each number, check if it is odd\n",
    "    if n % 2 == 1: # is it odd?\n",
    "        result = result + n\n",
    "print(result)"
   ]
